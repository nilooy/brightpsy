{
  "name": "psicologi",
  "private": true,
  "scripts": {
    "start": "MONGO_URL=mongodb://localhost:27017/psicologi meteor run",
    "test": "meteor test --once --driver-package meteortesting:mocha",
    "test-app": "TEST_WATCH=1 meteor test --full-app --driver-package meteortesting:mocha",
    "visualize": "meteor --production --extra-packages bundle-visualizer",
    "deploy": "cd .deploy && mup deploy --config=mup-local.js",
    "deploy-prod": "cd .deploy && mup deploy",
    "predeploy-staging": "cd .deploy && mup setup --config=mup-staging.js",
    "deploy-staging": "mup deploy --config=mup-staging.js"
  },
  "meteor": {
    "mainModule": {
      "client": "imports/main.js",
      "server": "server/main.js"
    },
    "testModule": "tests/main.js"
  },
  "dependencies": {
    "@babel/runtime": "^7.11.2",
    "@react-icons/all-files": "^4.1.0",
    "@windmill/react-ui": "0.3.1",
    "bcrypt": "^5.0.0",
    "chart.js": "2.9.3",
    "classnames": "2.2.6",
    "faker": "4.1.0",
    "firebase": "^8.2.0",
    "imports": "^1.0.0",
    "meteor-node-stubs": "^1.0.1",
    "react": "^16.13.1",
    "react-chartjs-2": "2.9.0",
    "react-dom": "^16.13.1",
    "react-focus-lock": "2.4.0",
    "react-helmet": "^6.1.0",
    "react-meteor-data": "^0.2.11",
    "react-router-dom": "5.2.0",
    "react-scripts": "3.4.1",
    "react-ssr-prepass": "^1.2.1",
    "react-transition-group": "4.4.1"
  },
  "devDependencies": {
    "mup": "^1.5.3",
    "@babel/runtime": "^7.11.2",
    "@svgr/webpack": "5.4.0",
    "autoprefixer": "^10.1.0",
    "commitizen": "4.1.2",
    "cross-env": "7.0.2",
    "cssnano": "4.1.10",
    "cz-conventional-changelog": "3.2.0",
    "meteor-node-stubs": "^1.0.1",
    "postcss": "^8.2.1",
    "postcss-load-config": "^3.0.0",
    "tailwindcss": "^2.0.2"
  }
}
